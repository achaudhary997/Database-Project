<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="1000.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.InsertController">
    <children>
        <TextArea fx:id="queryTextArea" layoutX="215.0" layoutY="380.0" prefHeight="81.0" prefWidth="771.0" />
        <Button fx:id="queryButton" layoutX="570.0" layoutY="485.0" mnemonicParsing="false" onAction="#showQuery" text="Query" />
        <TableView fx:id="queryResultTable" layoutX="215.0" layoutY="534.0" prefHeight="445.0" prefWidth="771.0">
            <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
            </columns>
        </TableView>
        <Label layoutX="165.0" layoutY="44.0" text="Table Name" />
        <Label layoutX="868.0" layoutY="44.0" text="Enter Values (comma separated)" />
        <TextField fx:id="tableName" layoutX="111.0" layoutY="87.0" />
        <Button fx:id="parse" layoutX="538.0" layoutY="328.0" mnemonicParsing="false" onAction="#parseQuery" text="Show my Query" />
      <Label layoutX="502.0" layoutY="44.0" text="Attributes (comma separated)" />
      <TextArea fx:id="insAttributes" layoutX="501.0" layoutY="87.0" prefHeight="81.0" prefWidth="200.0" />
      <TextArea fx:id="insValues" layoutX="875.0" layoutY="87.0" prefHeight="81.0" prefWidth="200.0" />
    </children>

</AnchorPane>
